{
  "manifest_version": 3,
  "name": "Keyword Extractor",
  "version": "1.0",
  "description": "Extracts keywords from the current webpage",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["JSDOMParser.js", "Readability.js", "content.js"]
    }
]

}
